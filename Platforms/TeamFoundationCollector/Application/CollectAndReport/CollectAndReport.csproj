<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>10.0</LangVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyName>Ban3.Platforms.TeamFoundationCollector.Application.CollectAndReport</AssemblyName>
    <RootNamespace>Ban3.Platforms.TeamFoundationCollector.Application.CollectAndReport</RootNamespace>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Ban3.TeamFoundationCollector.CollectAndReport</Title>
    <Version>1.0.0</Version>
    <Authors>zhaoma</Authors>
    <Company>BAN3</Company>
    <Description>TeamFoundation Collect and Report</Description>
    <PackageIcon>icon.png</PackageIcon>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <ApplicationIcon>favicon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="favicon.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\..\icon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Ban3.Infrastructures.NetMail" Version="1.0.0" />
    <PackageReference Include="Ban3.Infrastructures.PlatformInvoke" Version="1.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\..\Libs\SpringConfig\SpringConfig.csproj" />
    <ProjectReference Include="..\..\..\..\Libs\WxsConfig\WxsConfig.csproj" />
    <ProjectReference Include="..\..\Domain\Contract\Contract.csproj" />
  </ItemGroup>

</Project>
