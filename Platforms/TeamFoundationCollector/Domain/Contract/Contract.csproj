<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyName>Ban3.Platforms.TeamFoundationCollector.Domain.$(MSBuildProjectName)</AssemblyName>
    <RootNamespace>Ban3.Platforms.TeamFoundationCollector.Domain.$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Extensions\" />
    <Folder Include="Response\Core\" />
    <Folder Include="Response\Pipelines\" />
    <Folder Include="Settings\" />
    <Folder Include="Request\Core\" />
    <Folder Include="Request\Dashboard\" />
    <Folder Include="Request\SubCondition\" />
    <Folder Include="Request\Pipelines\" />
    <Folder Include="Request\WorkItemTracking\" />
    <Folder Include="Request\WorkItemTrackingProcess\" />
    <Folder Include="Request\Discussion\" />
    <Folder Include="Response\Discussion\" />
    <Folder Include="Models\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
    <PackageReference Include="log4net" Version="2.0.15" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\..\..\Libs\Common\Common.csproj" />
    <ProjectReference Include="..\..\..\..\Libs\NetHttp\NetHttp.csproj" />
    <ProjectReference Include="..\..\..\..\Libs\RuntimeCaching\RuntimeCaching.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Request\Pipelines\" />
    <None Remove="Interfaces\Functions\" />
    <None Remove="Request\WorkItemTracking\" />
    <None Remove="Request\WorkItemTrackingProcess\" />
    <None Remove="Request\Discussion\" />
    <None Remove="Response\Discussion\" />
    <None Remove="Models\" />
  </ItemGroup>
</Project>
