<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>10.0</LangVersion>
    <Nullable>enable</Nullable>
    <AssemblyName>Ban3.Platforms.TeamFoundationCollector.Domain.Contract</AssemblyName>
    <RootNamespace>Ban3.Platforms.TeamFoundationCollector.Domain.Contract</RootNamespace>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Ban3.TeamFoundationCollector.Contract</Title>
    <Version>1.0.0</Version>
    <Authors>zhaoma</Authors>
    <Company>BAN3</Company>
    <Description>TeamFoundation Collector Contract</Description>
    <PackageIcon>icon.png</PackageIcon>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <ApplicationIcon>favicon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\..\icon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Ban3.Infrastructures.Common" Version="1.0.2" />
    <PackageReference Include="Ban3.Infrastructures.NetHttp" Version="1.0.0" />
    <PackageReference Include="Ban3.Infrastructures.RuntimeCaching" Version="1.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="7.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="7.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="7.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="log4net" Version="2.0.15" />
    <PackageReference Include="NPOI" Version="2.6.0" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Response\Reports\" />
  </ItemGroup>
</Project>
