<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>10.0</LangVersion>
    <AssemblyName>Ban3.Productions.Casino.CcaAndReport</AssemblyName>
    <RootNamespace>Ban3.Productions.Casino.CcaAndReport</RootNamespace>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <DebugType>embedded</DebugType>
    <Title>Ban3.Productions.Casino.CcaAndReport</Title>
    <Version>1.0.0</Version>
    <Authors>zhaoma</Authors>
    <Company>BAN3</Company>
    <Description>Casino Collect , Calculate,Analyze and Report</Description>
    <PackageIcon>icon.png</PackageIcon>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <ApplicationIcon>favicon.ico</ApplicationIcon>
    <BaseOutputPath>D:\GIT\output</BaseOutputPath>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Models\" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\..\..\icon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Ban3.Productions.Casino.Contracts" Version="1.0.3" />
  </ItemGroup>
</Project>
