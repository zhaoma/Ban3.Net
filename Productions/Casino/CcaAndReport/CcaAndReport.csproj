<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageLicenseExpression>WTFPL</PackageLicenseExpression>
        <LangVersion>10.0</LangVersion>
        <AssemblyName>Ban3.Productions.Casino.CcaAndReport</AssemblyName>
        <RootNamespace>Ban3.Productions.Casino.CcaAndReport</RootNamespace>
        <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
        <DebugType>embedded</DebugType>
        <Title>Ban3.Productions.Casino.CcaAndReport</Title>
        <Version>1.0.4</Version>
        <Authors>zhaoma</Authors>
        <Company>BAN3</Company>
        <Description>Casino Collect , Calculate,Analyze and Report</Description>
        <PackageIcon>icon.png</PackageIcon>
        <AssemblyVersion>1.0.0.4</AssemblyVersion>
        <PackageReadmeFile>readme.md</PackageReadmeFile>
        <ApplicationIcon>favicon.ico</ApplicationIcon>
        <Configurations>Release;Debug</Configurations>
        <BaseOutputPath>..\..\output</BaseOutputPath>
    </PropertyGroup>

    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
        <OutputPath>..\..\..\output\</OutputPath>
        <DocumentationFile>..\..\..\output\xmls\Ban3.Productions.Casino.CcaAndReport.xml</DocumentationFile>
        <AllowedReferenceRelatedFileExtensions>
            .allowedextension
        </AllowedReferenceRelatedFileExtensions>
    </PropertyGroup>

    <ItemGroup>
        <Content Include="favicon.ico" />
        <None Include="..\..\..\icon.png">
            <Pack>True</Pack>
            <PackagePath>\</PackagePath>
        </None>
        <None Update="readme.md">
            <Pack>True</Pack>
            <PackagePath>\</PackagePath>
        </None>
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Ban3.Productions.Casino.Contracts" Version="1.0.26" />
    </ItemGroup>
</Project>