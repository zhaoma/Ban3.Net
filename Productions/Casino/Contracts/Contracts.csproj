<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageLicenseExpression>WTFPL</PackageLicenseExpression>
    <LangVersion>10.0</LangVersion>
    <AssemblyName>Ban3.Productions.Casino.Contracts</AssemblyName>
    <RootNamespace>Ban3.Productions.Casino.Contracts</RootNamespace>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <DebugType>embedded</DebugType>
    <Title>Ban3.Productions.Casino.Contracts</Title>
    <Version>1.0.20</Version>
    <Authors>zhaoma</Authors>
    <Company>BAN3</Company>
    <Description>Casino Contracts</Description>
    <PackageIcon>icon.png</PackageIcon>
    <AssemblyVersion>1.0.13.20</AssemblyVersion>
    <ApplicationIcon>favicon.ico</ApplicationIcon>
    <PackageReadmeFile>readme.md</PackageReadmeFile>
    <Configurations>Release;Debug</Configurations>
  </PropertyGroup>
  
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <OutputPath>..\..\..\output\</OutputPath>
    <DocumentationFile>..\..\..\output\Ban3.Infrastructures.Common.xml</DocumentationFile>
    <AllowedReferenceRelatedFileExtensions>
      .allowedextension
    </AllowedReferenceRelatedFileExtensions>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="favicon.ico" />
    <None Include="..\..\..\icon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Update="readme.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Ban3.Infrastructures.Indicators" Version="1.0.10" />
    <PackageReference Include="Ban3.Infrastructures.NetHttp" Version="1.0.6" />
    <PackageReference Include="Ban3.Infrastructures.RuntimeCaching" Version="1.0.6" />
    <PackageReference Include="Ban3.Sites.ViaEastmoney" Version="1.0.2" />
    <PackageReference Include="Ban3.Sites.ViaNetease" Version="1.0.2" />
    <PackageReference Include="Ban3.Sites.ViaSina" Version="1.0.2" />
    <PackageReference Include="Ban3.Sites.ViaTushare" Version="1.0.4" />
    <PackageReference Include="Ban3.Sites.ViaYuncaijing" Version="1.0.2" />
  </ItemGroup>
</Project>
