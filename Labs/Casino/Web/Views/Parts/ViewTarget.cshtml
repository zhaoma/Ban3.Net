@using Ban3.Infrastructures.Indicators
@model Ban3.Productions.Casino.Contracts.Models.Target
@{
    var dic = Model.LatestSets?.SetKeys.SetsKeysSummary();
}
<div class="container">
    <div class="col">
        <h1>@Model.Stock.Name
                <sub>@Html.Raw(Model.Html())</sub>
        </h1>
        <ol>
            @foreach (var p in Model.Points)
            {
            <li>@Html.Raw(p.Html(Model.PreClose()))</li>
            }
        </ol>
        <hr />
        <div class="targetButtons">
            <button type="button" onclick="return viewTarget('@Model.Stock.Code',1);" class="btn btn-warning">Ignore</button>
            <button type="button" onclick="return viewTarget('@Model.Stock.Code',0);" class="btn btn-primary">Skip</button>
        </div>
        <ol>
            @if (dic != null)
            {
                foreach (var kv in dic)
                {
                    <li>@kv.Key : @kv.Value</li>
                }
            }
        </ol>
        </div>
<div class="row">
    <div class="col-12 lazyLoad" id="onesChartsContainer" dataUrl="/parts/stock/@Model.Stock.Code"></div>
</div>
</div>


